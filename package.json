{
  "name": "@epiijs/website",
  "version": "1.0.0",
  "description": "The website for epiijs.",
  "main": "build/start.js",
  "scripts": {
    "build-script": "tsc --project tsconfig.script.json",
    "start-server": "tsc --project tsconfig.server.json && node build/script/start-server.js",
    "start-client": "tsc --project tsconfig.client.json && node build/script/start-client.js",
    "start": "npm run build-script && npm run start-server && npm run start-client"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@epiijs/client": "file:../client",
    "@epiijs/eslint-config": "^0.1.3",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.41.0"
  },
  "dependencies": {
    "@epiijs/config": "^0.2.1",
    "@epiijs/server": "file:../server",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
